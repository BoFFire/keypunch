#: data/dev.bragefuglseth.Keypunch.desktop.in.in:10
msgid ""
"typing;trainer;touch typing;keyboard;speed;wpm;words per minute;accuracy;"
"test;"
msgstr ""
"Tippen;Trainer;Tippen mit dem Finger;Tastatur;Geschwindigkeit;wpm;Wörter pro "
